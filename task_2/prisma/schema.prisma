generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Review {
  id              String   @id @default(uuid())
  rating          Int
  userReview      String
  aiResponse      String
  aiSummary       String
  aiActions       String
  createdAt       DateTime @default(now())
}
